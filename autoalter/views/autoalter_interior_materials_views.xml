<odoo>
    <record id="autoalter_interior_materials_view_tree" model="ir.ui.view">
        <field name="name">autoalter.interior.materials.tree</field>
        <field name="model">autoalter.interior.materials</field>
        <field name="arch" type="xml">
            <tree string="Interior Materials">
                <field name="interior_mat"/>
            </tree>
        </field>
    </record>
    <record id="autoalter_interior_materials_view_form" model="ir.ui.view">
        <field name="name">autoalter.interior.materials.form</field>
        <field name="model">autoalter.interior.materials</field>
        <field name="arch" type="xml">
            <form string="Interior Materials">
                <sheet>
                    <h1>
                        <group>
                            <field name="interior_mat"/>
                        </group>
                    </h1>
                </sheet>
            </form>
        </field>
    </record>
</odoo>